{
    "name": "PushD PopD",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
      "16": "save16.png", 
      "48": "save48.png", 
      "128": "save.png"
    },
    "permissions": [
      "tabs", "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "commands": {
      "pushd": {
        "suggest_key": {
          "default": "Ctrl+Shift+."
        },
        "description": "Push URL"
      },
      "popd": {
        "suggest_key": {
          "default": "Ctrl+Shift+,"
        },
        "description": "Pop URL"
      }
    }
}
